{
  "manifest_version": 3,
  "name": "chrome-catalogue",
  "version": "1.0.0",
  "description": "chrome目录提取",
  "icons": {
    "16": "img/logo.jpg",
    "48": "img/logo.jpg",
    "128": "img/logo.jpg"
  },
  "action": {
    "default_title": "chrome-catalogue",
    "default_icon": "img/logo.jpg",
    "default_popup": "popup/dist/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "bg.js",
        "contentScript.js"
      ],
      "run_at": "document_end",
      "css": [
        "myStyle.css",
        "panel/dist/css/panel.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "panel/dist/js/app.b18b6ffd.js",
        "panel/dist/js/chunk-vendors.6e3bdfb6.js",
        "combinAllJs.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}