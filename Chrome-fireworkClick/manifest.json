{
    "name": "烟花点击特效",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "烟花点击特效",
    "browser_action": {
        "default_title": "查看",
        "default_icon": "1.jpg"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["firework.js"]
        }
    ],
    "permissions" : ["tabs", "activeTab"] //向浏览器申请的权限
}